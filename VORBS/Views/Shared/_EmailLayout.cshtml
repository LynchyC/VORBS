</head>
<body>
    <table cellpadding="0" cellspacing="0" border="0" id="backgroundTable">
        <tr>
            <td valign="top">
                <table cellpadding="0" cellspacing="0" border="0" align="center" width="580" style="background-color: #0b0c0c;">
                    <tr>
                        <td width="20"></td>
                        <td width="560" valign="middle" height="56">
                            <img class="image_fix" src="cid:govuklogo" alt="GOV.UK" title="GOV.UK Image" width="159" height="36" />
                        </td>
                    </tr>
                </table>
                <table cellpadding="0" cellspacing="0" border="0" align="center" width="580">
                    <tr>
                        <td width="20"></td>
                        <td width="560" valign="middle" height="56">
                            <img class="image_fix" src="cid:voalogo" alt="Valuation Office Agency" title="Valuation Office Agency Image" width="313" height="37" />
                        </td>
                    </tr>
                </table>

                <table cellpadding="0" cellspacing="0" border="0" align="center">
                    <tr>
                        <td width="20"></td>
                        <td width="540" valign="top">

                            @RenderBody()

                        </td>
                        <td width="20"></td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <!-- End of wrapper table -->
</body>
</html>
